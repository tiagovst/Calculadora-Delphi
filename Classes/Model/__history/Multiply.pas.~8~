unit Multiply;

interface

uses System.Generics.Collections, Model.Calc.Interfaces;

type
  TMultiply = class
    private
      FList: TList<Integer>;
      FTotal: Integer;
    public
      constructor Create;
      function Execute: Integer;
      property ValueList: TList<Integer> read FList write Flist;
  end;

implementation


constructor TMultiply.Create;
begin
  FList := TList<Integer>.Create;
end;

function TMultiply.Execute: Integer;
var
  index: Integer;
begin
  FTotal := FList[0];
  for index := 0 to Pred(FList.Count) do
    FTotal := FTotal * FList[index];

  result := FTotal;
end;

end.
