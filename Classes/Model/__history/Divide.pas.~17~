unit Divide;

interface

uses System.Generics.Collections, Model.Calc.Interfaces;

type
  TDivide = class
    private
      FList: TList<Integer>;
      FTotal: Integer;
    public
      constructor Create;
      function Execute: Integer;
      property ValueList: TList<Integer> read FList write Flist;
  end;

implementation


constructor TDivide.Create;
begin
  FList := TList<Integer>.Create;
end;

function TDivide.Execute: Integer;
var
  index: Integer;
begin
  if FList.Count > 0 then
    FTotal := FList[0]
  else
    FTotal := 0;

  for index := 0 to Pred(FList.Count) do
    FTotal := FTotal div FList[index];

  result := FTotal;
end;

end.
